<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#626767">

    <Button
        android:id="@+id/whatapp123"
        android:layout_width="132dp"
        android:layout_height="133dp"
        android:layout_marginStart="46dp"
        android:layout_marginTop="37dp"
        android:layout_marginEnd="233dp"
        android:layout_marginBottom="561dp"
        android:onClick="Whatsapp"
        android:text="Whats App"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/Fb123"
        android:layout_width="132dp"
        android:layout_height="133dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="39dp"
        android:layout_marginEnd="57dp"
        android:layout_marginBottom="559dp"
        android:onClick="Fb"
        android:text="Fb"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/whatapp123"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/discord123"
        android:layout_width="132dp"
        android:layout_height="133dp"
        android:layout_marginStart="46dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="233dp"
        android:layout_marginBottom="378dp"
        android:onClick="discord"
        android:text="Discord"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/whatapp123" />

    <Button
        android:id="@+id/instagram123"
        android:layout_width="132dp"
        android:layout_height="133dp"
        android:layout_marginStart="42dp"
        android:layout_marginTop="47dp"
        android:layout_marginEnd="50dp"
        android:layout_marginBottom="377dp"
        android:onClick="instagram"
        android:text="Instagram"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/discord123"
        app:layout_constraintTop_toBottomOf="@+id/Fb123" />

    <Button
        android:id="@+id/linkedin123"
        android:layout_width="132dp"
        android:layout_height="133dp"
        android:layout_marginStart="46dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="233dp"
        android:layout_marginBottom="195dp"
        android:onClick="linkedin"
        android:text="Linkedin"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/discord123"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/youtube123"
        android:layout_width="132dp"
        android:layout_height="133dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="49dp"
        android:layout_marginEnd="57dp"
        android:layout_marginBottom="195dp"
        android:onClick="youtube"
        android:text="You Tube "
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/linkedin123"
        app:layout_constraintTop_toBottomOf="@+id/instagram123" />

    <Button
        android:id="@+id/message123"
        android:layout_width="132dp"
        android:layout_height="133dp"
        android:layout_marginStart="46dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="233dp"
        android:layout_marginBottom="12dp"
        android:onClick="message"
        android:text="Message"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linkedin123" />

    <Button
        android:id="@+id/contacts123"
        android:layout_width="132dp"
        android:layout_height="133dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="57dp"
        android:layout_marginBottom="12dp"
        android:onClick="contacts"
        android:text="Contacts"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/message123"
        app:layout_constraintTop_toBottomOf="@+id/youtube123" />


</androidx.constraintlayout.widget.ConstraintLayout>